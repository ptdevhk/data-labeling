    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^10.0.4",
    "vite": "^7.1.12",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-dts": "^4.5.4",

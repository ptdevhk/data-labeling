  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://karlorz.github.io/react-image-annotate",
  "repository": {
    "type": "git",
    "url": "https://github.com/karlorz/react-image-annotate.git"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build && node scripts/prepare-dist.js",
    "preview": "vite preview",
    "dist": "npm run build && cd dist && npm publish",
    "storybook": "storybook dev -p 9090",
    "build-storybook": "storybook build",
    "build:gh-pages": "vite build --mode site && mkdir -p dist/demo && cp dist/index.html dist/demo/index.html",
    "gh-pages": "npm run build:gh-pages && gh-pages -d dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:test": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:i18n": "node scripts/i18n-helper.js",
    "postinstall": "patch-package"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",

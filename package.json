{
  "name": "@karlorz/react-image-annotate",
  "version": "4.0.7",
  "type": "module",
  "description": "A powerful React component for image and video annotation with bounding boxes, polygons, points, and more",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./headless": {
      "types": "./dist/headless.d.ts",
      "import": "./dist/headless.js",
      "require": "./dist/headless.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": "karlorz",
  "license": "Apache-2.0",
